        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AdidasBot.Windows"
        mc:Ignorable="d"
        Title="Assing Proxies" Height="354" Width="319.211" ShowInTaskbar="False" ResizeMode="NoResize" WindowStartupLocation="CenterScreen">
