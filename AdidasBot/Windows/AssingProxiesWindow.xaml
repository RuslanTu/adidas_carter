        <DataGrid x:Name="dataGridAssingProxy" HorizontalAlignment="Left" Margin="10,28,0,0" VerticalAlignment="Top" Height="261" Width="293"/>
        <TextBlock x:Name="textBlock" HorizontalAlignment="Left" Margin="10,10,0,0" TextWrapping="Wrap" Text="Assign Proxy to:" VerticalAlignment="Top"/>
        <TextBlock x:Name="textBlockProxyTo" HorizontalAlignment="Left" Margin="113,10,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="171"/>
        <Button x:Name="buttonAutoAssingProxies" Content="Auto Assing" HorizontalAlignment="Left" Margin="10,294,0,0" VerticalAlignment="Top" Width="134" Click="buttonAutoAssingProxies_Click"/>
        <Button x:Name="buttonManualAssingProxy" Content="Manual Assing" HorizontalAlignment="Left" Margin="169,294,0,0" VerticalAlignment="Top" Width="134" Click="buttonManualAssingProxy_Click"/>

    </Grid>
</Window>
