<Window x:Class="AdidasCarterPro.Windows.CefBrowserWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AdidasCarterPro.Windows"
        xmlns:cefSharp="clr-namespace:CefSharp.Wpf;assembly=CefSharp.Wpf"
        mc:Ignorable="d"
        Title="CefBrowserWindow" Height="307.732" Width="688.144">
    <Grid>
        <Button x:Name="button" Content="Show Cookies" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Width="108" Click="button_Click"/>
        <cefSharp:ChromiumWebBrowser Name="browser1" Address="https://google.com" Margin="0,42,0,0" LoadingStateChanged="browser1_LoadingStateChanged" FrameLoadEnd="browser1_FrameLoadEnd"/>
        <Button x:Name="button1" Content="Check my IP" HorizontalAlignment="Left" Margin="134,10,0,0" VerticalAlignment="Top" Width="110" Click="button1_Click"/>
    </Grid>
</Window>
